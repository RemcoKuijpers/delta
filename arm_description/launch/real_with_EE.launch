<launch>
  <arg
    name="model" />
  <arg
    name="gui"
    default="False" />
  <param name="robot_description" command="$(find xacro)/xacro  --inorder  '$(find arm_description)/urdf/arm_with_endeffector.urdf.xacro'" />
  <param
    name="use_gui"
    value="$(arg gui)" />
  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="robot_state_publisher" />
</launch>
